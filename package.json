{
  "name": "form-wizard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.0",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "NODE_PATH=./src  react-scripts start",
    "build": "react-scripts build",
    "jest": "NODE_PATH=./src jest",
    "cypress": "start-server-and-test start http://localhost:3000 cypress:run",
    "test": "npm run pretty && npm run lint && npm run jest && npm run cypress",
    "eject": "react-scripts eject",
    "pretty": "npx prettier --write ./src",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx && npx prettier --check ./src && tsc --noEmit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.8.0",
    "eslint": "8.22.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
